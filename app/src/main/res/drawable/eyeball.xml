<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Larger oval in back -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#FFFFFF"/>
            <size
                android:width="25dp"
                android:height="25dp"/>
            <stroke
                android:width="3dp"
                android:color="#000000"/>
        </shape>
    </item>

    <!-- Smaller circle in front -->
    <item android:height="75dp" android:width="75dp" android:gravity="center">
        <shape
            android:shape="oval">
            <solid
                android:color="#000000"/>
        </shape>
    </item>
</layer-list>